# Docker Environment Variables Example
# Copy this to .env for local Docker development

# Backend Environment Variables (from src/backend/.env)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
B2_APPLICATION_KEY_ID=your_b2_key_id_here
B2_APPLICATION_KEY=your_b2_application_key_here
B2_BUCKET_NAME=your_b2_bucket_name_here
B2_ENDPOINT_URL=your_b2_endpoint_url_here
B2_BASE_URL=your_b2_base_url_here
B2_AUDIO_FOLDER=audio
B2_THUMBNAIL_FOLDER=thumbnails
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Frontend Environment Variables (only what's needed)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NODE_ENV=development
