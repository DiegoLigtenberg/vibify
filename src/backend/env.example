# Backend Environment Variables Example
# Copy this to .env and replace with your actual credentials

B2_ENDPOINT_URL="https://s3.your-region.backblazeb2.com"
B2_API_URL="https://api.backblazeb2.com"
B2_APPLICATION_KEY_ID="your_b2_key_id_here"
B2_APPLICATION_KEY="your_b2_application_key_here"
B2_BUCKET_NAME="your_b2_bucket_name_here"
B2_AUDIO_FOLDER="audio"
B2_THUMBNAIL_FOLDER="thumbnails"
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key_here"
SUPABASE_ANON_KEY="your_supabase_anon_key_here"
CORS_ORIGINS="https://your-production-domain.com,http://localhost:3000,http://127.0.0.1:3000"
PYTHON_ENV="development"
